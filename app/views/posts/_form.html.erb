<%= form_with model: post, local: true do |f| %>
  <%= render 'shared/error_messages', post: post %>
  <div class="form-group">
    <%= f.label :lyricks %>
    <%= f.text_area :lyricks, class: 'form-control', rows: 10 %>
  </div>
  <div class="form-group">
    <%= f.label :song_title %>
    <%= f.text_field :song_title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :artist %>
    <%= f.text_field :artist, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :link_to_music %>
    <%= f.text_field :link_to_music, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :design %>
    <%= f.text_field :design, class: 'form-control' %>
  </div>
  <br>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
