 <%= form_with model: post, local: true do |f| %>
   <div class="form-group">
     <%= f.label :song_titile %>
     <%= f.text_field :song_title, class: 'form-control' %>
   </div>
   <div class="form-group">
     <%= f.label :lyricks %>
     <%= f.text_area :lyricks, class: 'form-control', rows: 10 %>
   </div>

   <%= f.submit class: 'btn btn-primary' %>
 <% end %>
