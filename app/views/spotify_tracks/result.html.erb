<h1><%= t('defaults.artist') %> - <%= @artist_names %></h1>
<h1><%= t('defaults.track') %> - <%= @track_names %></h1>
<br>
<ul class="tracks-container">
  <% @track_urls.each do |url| %>
    <% track_id = url.split("/").last %>
    <li>
      <iframe src="https://open.spotify.com/embed/track/<%= track_id %>" width="300" height="80" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
    </li>
  <% end %>
</ul>
